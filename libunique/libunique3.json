{
  "name": "libunique3",
  "sources": [
    {
      "type": "archive",
      "url": "http://download.gnome.org/sources/libunique/3.0/libunique-3.0.2.tar.xz",
      "sha256": "a8f02ce073e2b920cca8ac45d418e7cb64438ad0814780c5912c6d63f8a4e038"
    },
    {
      "type": "script",
      "commands": [
        "autoreconf -sfi"
      ],
      "dest-filename": "autogen.sh"
    }
  ],
  "rm-configure": true,
  "config-opts": [
    "--disable-gtk-doc",
    "--disable-static",
    "--disable-introspection"
  ],
  "cleanup": [
    "/include",
    "/lib/*.la",
    "/lib/pkgconfig",
    "/share/gtk-doc"
  ]
}
